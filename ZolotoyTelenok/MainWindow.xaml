<Window x:Class="ZolotoyTelenok.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZolotoyTelenok"
        mc:Ignorable="d"
        Title="Золотой Теленок" WindowStartupLocation="CenterScreen" Width="1000" Height="1000">
    <Grid Background="#FFE3E3E3">
        <Grid.RowDefinitions>
            <RowDefinition Height="31*"/>
            <RowDefinition Height="215*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="47*"/>
            <ColumnDefinition Width="153*"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Grid.Column="0" Grid.Row="1"></Frame>
        <Frame x:Name="InfoFrame" NavigationUIVisibility="Hidden" Grid.Column="1" Grid.RowSpan="2"></Frame>
    </Grid>
</Window>
